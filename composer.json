{
    "name": "dende/magento",
    "description": "A simple Magento Integration using SOAP v1 and v2",
    "version": "1.0.0",
    "keywords": ["laravel", "magento", "soap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kyle Martin",
            "email": "michael@tinyrocket.co"
        },
	{
	    "name": "Christian Hartlage",
	    "email": "c@dende.de"
	}
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/Dende/Magento/Connections/Exceptions.php",
            "src/Dende/Magento/Objects/Exceptions.php"
        ],
        "psr-0": {
            "Dende\\Magento\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
